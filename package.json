{
  "name": "igata",
  "version": "1.3.3",
  "description": "Converts a JSON Schema to Flow type definition",
  "main": "lib/index.js",
  "scripts": {
    "compile": "$(npm bin)/tsc -p tsconfig.json --noEmit",
    "lint": "$(npm bin)/tslint src/**/*.ts test/**/*.ts",
    "lint:fix": "$(npm bin)/tslint --fix src/**/*.ts test/**/*.ts",
    "build": "$(npm bin)/webpack --config webpack.config.js",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test $(npm bin)/mocha --require ts-node/register --recursive test/*-test.ts test/**/*-test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sakai-akinobu/igata.git"
  },
  "keywords": [
    "flow",
    "json",
    "json schema",
    "type"
  ],
  "author": "Akinobu SAKAI <sakai.akinobu@gmail.com> (https://github.com/sakai-akinobu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sakai-akinobu/igata/issues"
  },
  "homepage": "https://github.com/sakai-akinobu/igata#readme",
  "pre-commit": [
    "lint",
    "compile",
    "test"
  ],
  "devDependencies": {
    "@types/babel__generator": "^7.0.1",
    "@types/mocha": "^5.2.5",
    "@types/power-assert": "^1.5.0",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.1",
    "pre-commit": "^1.2.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/generator": "^7.0.0",
    "@babel/types": "^7.0.0"
  }
}
